const readline = require("readline");

const inp = readline.createInterface({
  input: process.stdin
});

const userInput = [];

inp.on("line", (data) => {
  userInput.push(data);
});

inp.on("close", () => {
//a = userInput[0].split(" ")
//console.log(a)

(function(){
    
let arr = userInput[0].split(" ")

let odds = arr.filter(n => n%2)

console.log(odds)
})();
});

